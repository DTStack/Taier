  "scripts": {
    "start": "npm run dev",
    "dev": "node --max_old_space_size=4092 ./node_modules/.bin/webpack-dev-server --progress --inline --config build/dev.js --open",
    "dev:win": "./node_modules/.bin/webpack-dev-server --progress --inline --config build/dev.js --open",
    "build": "npm install --unsafe-perm=true --allow-root && rm -rf dist/* && webpack --profile --color --progress --config build/prod.js",
    "mock": "cup config",
    "test": "jest --no-cache",
    "release": "./scripts/release.sh",
    "ssh": "./scripts/ssh_server.sh",
    "hotfix": "./scripts/hotfix.sh --b",
    "push-dev": "./scripts/push-dev.sh",
    "scp": "./scripts/scp.sh",
    "check-types": "tsc --skipLibCheck",
    "lint": "npx eslint './src/**/*.ts' './src/**/*.tsx' './src/**/*.js'",
    "lint-doc": "node ./node_modules/lint-md/bin ./*.md docs/*.md",
    "lint-fix": "npx eslint './src/**/*.ts' './src/**/*.tsx' './src/**/*.js' --fix && lint-md ./*.md docs/*.md --fix",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md && npm run changeissueurl",
    "changeissueurl": "replace 'http://git.dtstack.cn/dtstack/data-stack-web/issues/' 'http://redmine.prod.dtstack.cn/issues/' CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -c ./commitlint.config.js -E HUSKY_GIT_PARAMS"
    }
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "npm run changeissueurl"
    }
  },
  "keywords": [
    "generator",
    "react"
