{
    "presets": [
        [
            "es2015",
            {
                "modules": false
            }
        ],
        "stage-0",
        "react"
    ],
    "plugins": [
        ["import", { "libraryName": "antd", "style": true }],
        "transform-runtime",
        "transform-decorators-legacy",
        "react-hot-loader/babel" // 开启 React 代码的模块热替换(HMR)
    ]
}