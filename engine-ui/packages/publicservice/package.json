{
  "name": "dt-public-service-front",
  "version": "4.2.0",
  "description": "public service",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "lint": "prettier --write . && eslint --ext .js,.jsx,.ts,.tsx --fix",
    "sonar": "node ./sonarcube.scanner.js",
    "check-types": "tsc --skipLibCheck",
    "dll": "ko dll",
    "dev": "ko dev -t",
    "build": "rm -rf dist && ko build -t",
    "swagger": "ko swagger -t",
    "prepare": "husky install",
    "test": "jest --coverage"
  },
  "dependencies": {
    "ant-design-dtinsight-theme": "1.1.2",
    "antd": "3.26.13",
    "base-64": "^1.0.0",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "^3.8.1",
    "dt-react-component": "1.3.0",
    "dt-utils": "1.0.1",
    "highlight.js": "^10.7.1",
    "history": "^5.0.0",
    "immutable": "~3.8.2",
    "ko-request": "^1.3.7",
    "lodash": "4.17.19",
    "mirror-creator": "1.1.0",
    "moment": "^2.29.1",
    "monaco-editor": "^0.24.0",
    "mxgraph": "4.0.6",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-hot-loader": "3.1.1",
    "react-loadable": "^5.5.0",
    "react-redux": "5.0.4",
    "react-resize-detector": "^5.2.0",
    "react-router": "3.0.4",
    "react-router-redux": "^4.0.8",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "redux-thunk": "2.2.0",
    "regenerator-runtime": "^0.13.7",
    "scheduler": "0.19.0",
    "styled-components": "3.1.6",
    "throttle-debounce": "^3.0.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/throttle-debounce": "^2.1.0",
    "commitizen": "^4.2.3",
    "copy-webpack-plugin": "^5.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-sonar-reporter": "2.0.0",
    "ko-script": "4.25.10",
    "prettier": "^2.2.1",
