<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dtstack.lineage.dao.LineageColumnColumnAppRefDao">

    <sql id="select_content_fragment">
      id,app_type,lineage_column_column_id,gmt_create,gmt_modified,is_deleted
    </sql>

    <insert id="batchInsert" parameterType="com.dtstack.engine.api.domain.LineageColumnColumnAppRef" useGeneratedKeys="true" keyProperty="id">
        insert into lineage_column_column_app_ref(app_type,lineage_column_column_id)
        values
         (#{appType},#{lineageColumnColumnId})
    </insert>

</mapper>
