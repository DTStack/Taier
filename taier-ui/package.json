		"react-copy-to-clipboard": "^5.1.0",
		"react-dom": "~18.2.0",
		"react-router-dom": "^6.0.2",
		"react-use": "^17.4.0",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.6.0",
